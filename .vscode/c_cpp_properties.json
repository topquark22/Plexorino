{
    "configurations": [
        {
            "name": "Arduino-Plexorino-Win",
            "includePath": [
                "${workspaceFolder}/src",
                "${workspaceFolder}",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/libraries/**",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/cores/arduino",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/variants/standard",
                "${env:ARDUINO15}/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/avr/include"
            ],
            "defines": [
                "F_CPU=16000000L",
                "ARDUINO=10607",
                "ARDUINO_AVR_NANO",
                "ARDUINO_ARCH_AVR",
                "__AVR_ATmega328P__"
            ],
            "compilerPath": "${env:ARDUINO15}/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-gcc.exe",
            "compilerArgs": [
                "-x",
                "c++",
                "-std=gnu++11",
                "-mmcu=atmega328p"
            ],
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/cores/arduino"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}