{
    "configurations": [
        {
            "name": "Arduino-Plexorino-Win",
            "includePath": [
                "${workspaceFolder}",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/cores/arduino",
                "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/variants/standard"
            ],
            "defines": [
                "F_CPU=16000000L",
                "ARDUINO"
            ],
            "compilerPath": "${env:ARDUINO15}/packages/arduino/tools/avr-gcc/7.3.0-atmel3.6.1-arduino7/bin/avr-gcc.exe",
            "intelliSenseMode": "gcc-x64",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "browse": {
                "path": [
                    "${workspaceFolder}",
                    "${env:ARDUINO15}/packages/arduino/hardware/avr/1.8.6/cores/arduino"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}